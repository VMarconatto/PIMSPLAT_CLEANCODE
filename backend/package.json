{
  "name": "typescript-rabbitmq",
  "version": "0.0.7",
  "description": "A wrapper library for using rabbitmq under typescript",
  "main": "dist/src/index.js",
  "types": "dist/src/index.d.ts",
  "directories": {
    "test": "tests"
  },
  "scripts": {
    "infra:up": "docker compose up -d --wait",
    "infra:down": "docker compose down",
    "infra:logs": "docker compose logs -f",
    "infra:logs:rabbit": "docker compose logs -f rabbitmq",
    "infra:ps": "docker compose ps",
    "build": "tsc",
    "start": "npm run start:stack",
    "start:collector": "node ./dist/telemetry/infrastructure/opcua/main.js",
    "start:consumer": "node ./dist/messaging/consumer.main.js",
    "start:api": "node ./dist/common/infrastructure/http/server.js",
    "start:stack": "node ./scripts/start-stack.js",
    "start:all": "npm run infra:up && npm run build && npm run start:collector",
    "start:api:all": "npm run infra:up && npm run build && npm run start:api"
    
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tsemach/typescript-rabbitmq.git"
  },
  "keywords": [
    "rabbitmq",
    "typescript",
    "amqplib"
  ],
  "author": "Tsemach Mizrachi <tsemach.mizrachi@gmail.com> (http://tsemach.github.io)",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/tsemach/typescript-rabbitmq/issues"
  },
  "homepage": "https://github.com/tsemach/typescript-rabbitmq#readme",
  "dependencies": {
    "@types/amqplib": "^0.5.17",
    "@types/assert": "0.0.31",
    "@types/bcryptjs": "^2.4.6",
    "@types/nodemailer": "^7.0.9",
    "@types/pino": "^7.0.4",
    "@types/uuid": "^3.4.3",
    "amqplib": "^0.5.2",
    "app-root-path": "^2.1.0",
    "assert": "^1.4.1",
    "axios": "^1.13.5",
    "bcryptjs": "^3.0.3",
    "class-transformer": "^0.5.1",
    "cors": "^2.8.6",
    "dotenv": "^17.2.4",
    "express": "^5.2.1",
    "express-async-errors": "^3.1.1",
    "fs-extra": "^11.3.3",
    "inversify": "^4.13.0",
    "jsonwebtoken": "^9.0.3",
    "logging": "^3.2.0",
    "mongodb": "^7.1.0",
    "mongoose": "^9.2.1",
    "node-opcua": "^2.163.1",
    "nodemailer": "^8.0.1",
    "pg": "^8.18.0",
    "pino": "^10.3.1",
    "reflect-metadata": "^0.1.14",
    "swagger": "^0.7.5",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "tsyringe": "^4.10.0",
    "typeorm": "^0.3.28",
    "uuid": "^3.3.2",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@types/chai": "^4.1.4",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.6",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/mocha": "^5.2.5",
    "@types/node": "^25.2.3",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.8",
    "chai": "^4.1.2",
    "mocha": "^5.2.0",
    "ts-node": "^7.0.0"
  }
}
